/*
 * Highlighted Icon 
 */
.higlighted-icon, .higlighted-icon-large {
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
  border-radius: 50%;
  -webkit-transition: all 0.25s ease-in-out;
  -moz-transition: all 0.25s ease-in-out;
  -ms-transition: all 0.25s ease-in-out;
  -o-transition: all 0.25s ease-in-out;
  transition: all 0.25s ease-in-out;
  border: 2px solid #337ab7;
  color: #337ab7;
  cursor: pointer;
  padding: 10px;
  width: 2em;
  text-align: center;
}

.higlighted-icon-large{
  padding: 34px;
  width: 104px;
}

.higlighted-icon:hover, .higlighted-icon-large:hover {
  color: #ffffff;
  background: #337ab7;
}

